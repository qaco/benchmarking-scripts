pluto.maxfuse=polycc --maxfuse
pluto.tiling.maxfuse=polycc --tile --maxfuse
pluto.tiling=polycc --tile
pluto.unroll4=polycc --unroll --ufactor 4
pluto.tiling.unroll=polycc --tile --unroll
